INTEGER FUNCTION NF_INQ_ENUM(INTEGER NCID, INTEGER XTYPE, CHARACTER*(*) NAME, INTEGER BASE_NF_TYPE, INTEGER BASE_SIZE, INTEGER NUM_MEMBERS)
